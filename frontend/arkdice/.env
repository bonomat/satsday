# API Base URL for the game backend
# This will be used to fetch game addresses and other data
VITE_API_BASE_URL=http://localhost:12345

# You can override this in your .env.local or during build time:
# VITE_API_BASE_URL=https://api.satsday.xyz

# Betting limits (in satoshis)
VITE_MIN_BET_SATS=100
VITE_MAX_BET_SATS=100000
